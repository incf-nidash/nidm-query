{
        "title": "HCLS Dataset Description Required",
        "description": "Required pieces of the HCLS dataset description version and distribution levels.",
        "creator": "<http://orcid.org/0000-0003-1099-3328>",
        "keyword": "",
        "parameters": [],
        "format": "ask",
        "model": "HCLS Dataset Description",
        "component": "dataset description",
        "sparql": ["PREFIX dcat: <http://www.w3.org/ns/dcat#>",
                   "PREFIX dct: <http://purl.org/dc/terms/>",
                   "PREFIX dctypes: <http://purl.org/dc/dcmitype/>",
                   "PREFIX foaf: <http://xmlns.com/foaf/0.1/>",
                   "PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>",
                   "PREFIX nidm: <http://purl.org/nidash/nidm#>",
                   "PREFIX niq: <http://purl.org/niquery#>",
                   "ASK",
                   "WHERE {",
                   "?uri a ?type ;",
                   "dct:title ?title ;",
                   "dct:description ?description ;",
                   "dct:publisher ?publisher ;",
                   "dct:hasPart ?hasPart .",
                   "OPTIONAL{ ?uri dct:isVersionOf ?isVersionOf . } # required at the version level",
                   "FILTER (!bound(?isVersionOf))",
                   "OPTIONAL{ ?uri dct:format ?format . } # required at the distribution level",
                   "FILTER (!bound(?isVersionOf))}"],
        "version":"",
        "reference": "",
        "@context":""

}
